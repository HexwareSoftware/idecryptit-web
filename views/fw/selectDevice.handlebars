{{> nav navFirmware=true}}

<div class="container-fluid">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item active" aria-current="page">Firmware Keys</li>
        </ol>
    </nav>
    <div class="row">
        <div class="col-sm-2">
            <div id="toc">
                <div id="tocTitle">Contents</div>
                <ul>
                    <li>1. <a href="#appletv">Apple TV</a></li>
                    <li>2. <a href="#ipad">iPad</a></li>
                    <li>3. <a href="#ipadmini">iPad mini</a></li>
                    <li>4. <a href="#ipadpro">iPad Pro</a></li>
                    <li>5. <a href="#iphone">iPhone</a></li>
                    <li>6. <a href="#ipodtouch">iPod touch</a></li>
                </ul>
            </div>
        </div>

        <div class="col-sm-10">
            <h1>Firmware Keys</h1>
            <p>
                If you don't know what device you have, please visit <a href="https://theiphonewiki.com/wiki/Models">the iPhone Wiki</a> to find out.
            </p>

            <h2 id="appletv">Apple TV</h2>
            {{#each appletv}}
            <a href="/fw/keys/appletv/{{id}}" class="fwDevice">
                <button type="button" class="btn btn-light">
                    <img src="/img/devices/appletv/{{image}}" alt="Apple TV {{varient}} (AppleTV{{id}})">
                    <br>
                    {{varient}} (AppleTV{{id}})
                </button>
            </a>
            {{/each}}

            <h2 id="ipad">iPad</h2>
            {{#each ipad}}
            <a href="/fw/keys/ipad/{{id}}" class="fwDevice">
                <button type="button" class="btn btn-light">
                    <img src="/img/devices/ipad/{{image}}" alt="iPad {{varient}} (iPad{{id}})">
                    <br>
                    {{varient}} (iPad{{id}})
                </button>
            </a>
            {{/each}}

            <h2 id="ipadmini">iPad mini</h2>
            {{#each ipadmini}}
            <a href="/fw/keys/ipad/{{id}}" class="fwDevice">
                <button type="button" class="btn btn-light">
                    <img src="/img/devices/ipadmini/{{image}}" alt="iPad mini {{varient}} (iPad{{id}})">
                    <br>
                    {{varient}} (iPad{{id}})
                </button>
            </a>
            {{/each}}

            <h2 id="ipadpro">iPad Pro</h2>
            {{#each ipadpro}}
            <a href="/fw/keys/ipad/{{id}}" class="fwDevice">
                <button type="button" class="btn btn-light">
                    <img src="/img/devices/ipadpro/{{image}}" alt="iPad Pro {{varient}} (iPad{{id}})">
                    <br>
                    {{varient}} (iPad{{id}})
                </button>
            </a>
            {{/each}}

            <h2 id="iphone">iPhone</h2>
            {{#each iphone}}
            <a href="/fw/keys/iphone/{{id}}" class="fwDevice">
                <button type="button" class="btn btn-light">
                    <img src="/img/devices/iphone/{{image}}" alt="iPhone {{varient}} (iPhone{{id}})">
                    <br>
                    {{varient}} (iPhone{{id}})
                </button>
            </a>
            {{/each}}

            <h2 id="ipodtouch">iPod touch</h2>
            {{#each ipodtouch}}
            <a href="/fw/keys/ipodtouch/{{id}}" class="fwDevice">
                <button type="button" class="btn btn-light">
                    <img src="/img/devices/ipodtouch/{{image}}" alt="iPod touch {{varient}} (iPod{{id}})">
                    <br>
                    {{varient}} (iPod{{id}})
                </button>
            </a>
            {{/each}}
        </div>
    </div>
</div>

{{> footer}}