{{> nav navFirmware=true}}

<div class="container-fluid">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/fw/keys">Firmware Keys</a></li>
            <li class="breadcrumb-item active" aria-current="page">{{device}}</li>
        </ol>
    </nav>
    <div class="row">
        <div class="col-sm-2">
            <!-- No TOC -->
        </div>

        <div class="col-sm-10">
            <h1>Firmware Keys: <code>{{device}}</code></h1>
            <p>
                Please select a firmware version:
            </p>
            <ul>
                {{#each keySets}}
                <li>
                    <a href="{{../urlpart}}/{{build}}">{{version}}: {{build}}</a>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
</div>

{{> footer}}